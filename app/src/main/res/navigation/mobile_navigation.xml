        android:label="@string/title_inbox"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="co.id.perluapps.playgroundfaiq.ui.notifications.NotificationsFragment"
